<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<!-- BlueberryPie -->

	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
			<li Class="ModCheck.isModLoaded">
				<modName>VGP Garden Gourmet</modName>
				<yourMod>No Water, No Life.</yourMod>
			</li>
			<li Class="PatchOperationTest">
				<xpath>*/ThingDef[defName = "BlueberryPie"]/comps</xpath>
				<success>Invert</success>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>*/ThingDef[defName = "BlueberryPie"]</xpath>
				<value>
					<comps/>
				</value>
			</li>
		</operations>
	</Operation>
	
	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
			<li Class="ModCheck.isModLoaded">
				<modName>VGP Garden Gourmet</modName>
				<yourMod>No Water, No Life.</yourMod>
			</li>
			<li Class="PatchOperationAdd">
			<xpath>*/ThingDef[defName = "BlueberryPie"]/comps</xpath>
				<value>
				<li Class="MizuMod.CompProperties_WaterSource">
					<sourceType>Item</sourceType>
					<getSound>Ingest_Beer</getSound>
					<baseDrinkTicks>100</baseDrinkTicks>
					<waterType>NormalWater</waterType>
					<needManipulate>true</needManipulate>
					<waterAmount>0.1</waterAmount>
					<maxNumToGetAtOnce>1</maxNumToGetAtOnce>
					<waterVolume>0</waterVolume>
				</li>
				</value>
			</li>
		</operations>
	</Operation>
	
